<fields>
  <field name="eml:eml">
    <suggest name="dataset"/>
  </field>

  <field name="dataset">
    <suggest name="alternateIdentifier"/>
    <suggest name="title"/>
    <suggest name="creator"/>
    <suggest name="metadataProvider"/>
    <suggest name="associatedParty"/>
    <suggest name="pubDate"/>
    <suggest name="abstract"/>
    <suggest name="keywordSet"/>
    <suggest name="additionalInfo"/>
    <suggest name="intellectualRights"/>
    <suggest name="distribution"/>
    <suggest name="coverage"/>
    <suggest name="purpose"/>
    <suggest name="contact"/>
  </field>

  <field name="creator">
    <suggest name="organizationName"/>
    <suggest name="individualName"/>
    <suggest name="positionName"/>
    <suggest name="address"/>
    <suggest name="phone"/>
    <suggest name="electronicMailAddress"/>
    <suggest name="onlineUrl"/>
  </field>

  <field name="metadataProvider">
    <suggest name="organizationName"/>
    <suggest name="individualName"/>
    <suggest name="positionName"/>
    <suggest name="address"/>
    <suggest name="phone"/>
    <suggest name="electronicMailAddress"/>
    <suggest name="onlineUrl"/>
  </field>

  <field name="associatedParty">
    <suggest name="organizationName"/>
    <suggest name="individualName"/>
    <suggest name="positionName"/>
    <suggest name="address"/>
    <suggest name="phone"/>
    <suggest name="electronicMailAddress"/>
    <suggest name="onlineUrl"/>
    <suggest name="role"/>
  </field>

  <field name="address">
    <suggest name="deliveryPoint"/>
    <suggest name="city"/>
    <suggest name="administrativeArea"/>
    <suggest name="postalCode"/>
    <suggest name="country"/>
  </field>

  <field name="distribution">
    <suggest name="online"/>
  </field>

  <field name="online">
    <suggest name="url"/>
  </field>

  <field name="contact">
    <suggest name="individualName"/>
    <suggest name="organizationName"/>
    <suggest name="positionName"/>
    <suggest name="address"/>
    <suggest name="phone"/>
    <suggest name="electronicMailAddress"/>
    <suggest name="onlineUrl"/>
  </field>

  <field name="individualName">
    <suggest name="givenName"/>
    <suggest name="surName"/>
  </field>

  <field name="description">
    <suggest name="para" />
  </field>

  <field name="descriptor">
    <suggest name="descriptorValue" />
  </field>

  <field name="studyAreaDescription">
    <suggest name="descriptor" />
  </field>

  <field name="designDescription">
    <suggest name="description" />
  </field>

  <field name="project">
    <suggest name="title"/>
    <suggest name="personnel"/>
    <suggest name="funding"/>
    <suggest name="studyAreaDescription"/>
    <suggest name="designDescription"/>
  </field>

  <field name="methods">
    <suggest name="methodStep"/>
    <suggest name="sampling"/>
  </field>

  <field name="methodStep">
      <suggest name="description"/>
  </field>

  <field name="sampling">
    <suggest name="studyExtent"/>
    <suggest name="samplingDescription"/>
  </field>

  <field name="studyExtent">
      <suggest name="description"/>
  </field>

  <field name="samplingDescription">
      <suggest name="para"/>
  </field>

</fields>
